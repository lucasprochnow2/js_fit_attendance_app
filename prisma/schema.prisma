generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-arm64-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?

  @@map("user")
}

model Classroom {
  id         Int      @id @default(autoincrement())
  name       String
  start_time DateTime @db.Time(6)
  end_time   DateTime @db.Time(6)

  @@map("classroom")
}
